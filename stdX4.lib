                        }
                	cell_rise(Propagation_Delay) {
                                index_1 ("1,  2");
                                index_2 ("2, 10");
                                values ("0.411,0.541","0.543,0.714");
                        }
                	fall_transition(Out_Transition) {
                                index_1 ("1,  2");
                                index_2 ("2, 10");
                                values ("0.149,0.188","0.228,0.294");
                        }
                	rise_transition(Out_Transition) { 
                                index_1 ("1,  2");
                                index_2 ("2, 10");
                                values ("0.191,0.332","0.275,0.418");
                        }
		} /* End Timing Related to Pin A */
		timing () {
      			related_pin    : "B";
			cell_fall(Propagation_Delay) {
                                index_1 ("1,  2");
                                index_2 ("2, 10");
                                values ("0.321,0.391","0.485,0.592");
                        }
                	cell_rise(Propagation_Delay) {
                                index_1 ("1,  2");
                                index_2 ("2, 10");
                                values ("0.411,0.541","0.543,0.714");
                        }
                	fall_transition(Out_Transition) {
                                index_1 ("1,  2");
                                index_2 ("2, 10");
                                values ("0.149,0.188","0.228,0.294");
                        }
                	rise_transition(Out_Transition) { 
                                index_1 ("1,  2");
                                index_2 ("2, 10");
                                values ("0.191,0.332","0.275,0.418");
			}
		} /* End Timing Related to Pin B */
		
	  } /* End of Pin Z*/
  } /*End of NOR Cell */

/******************************************************************************************
   Module           : AND_X4
   Cell Description : Combinational cell (AND_X4) with drive strength
X4
*******************************************************************************************/

 cell (AND_X4) {

        pg_pin(VDD) {
                voltage_name : VDD;
                pg_type      : primary_power;
        }
        pg_pin(VSS) {
                voltage_name : VSS;
                pg_type      : primary_ground;
        }

        drive_strength      : 4;
        area                : 44;
        
        cell_leakage_power      : 1.64;

        /* Input pins: No timing, but need to specify input capacitance
        to enable Timing Analyses */
        pin (A) {
                direction               : input;
                capacitance             : 16;
        }
        pin (B) {
                direction               : input;
                capacitance             : 16;
        }

        pin (Z) {
                direction   : output;  
                max_capacitance   : 19;
                function    : "(A & B)";
                
                timing () {
                        related_pin    : "A";
                        cell_fall(Propagation_Delay) {
                                index_1 ("1,  2");
                                index_2 ("2, 10");
                                values ("0.484,0.574","0.699,0.821");
                        }
                        cell_rise(Propagation_Delay) {
                                index_1 ("1,  2");
                                index_2 ("2, 10");
                                values ("0.520,0.633","0.707,0.847");
                        }
                        fall_transition(Out_Transition) {
                                index_1 ("1,  2");
                                index_2 ("2, 10");
                                values ("0.202,0.293","0.291,0.386");
                        }
                        rise_transition(Out_Transition) { 
                                index_1 ("1,  2");
                                index_2 ("2, 10");
                                values ("0.735,0.917","0.852,0.100");
                        }
                } /* End Timing Related to Pin A */
                timing () {
                        related_pin    : "B";
                        cell_fall(Propagation_Delay) {
                                index_1 ("1,  2");
                                index_2 ("2, 10");
                                values ("0.484,0.574","0.699,0.821");
                        }
                        cell_rise(Propagation_Delay) {
                                index_1 ("1,  2");
                                index_2 ("2, 10");
                                values ("0.520,0.633","0.707,0.847");
                        }
                        fall_transition(Out_Transition) {
                                index_1 ("1,  2");
                                index_2 ("2, 10");
                                values ("0.202,0.293","0.291,0.386");
                        }
                        rise_transition(Out_Transition) {
                                index_1 ("1,  2");
                                index_2 ("2, 10");
                                values ("0.735,0.917","0.852,0.100");
                        }
                } /* End Timing Related to Pin B */
        } /* End of Pin Z */
  } /* End of Cell AND */
} /*End of Library*/
